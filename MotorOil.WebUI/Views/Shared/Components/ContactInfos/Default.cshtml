@model IEnumerable<ContactInfo>




    @foreach (var item in Model)
{

    <div class="contact-page-side-content">
        <h3 class="contact-page-title">Bizimlə əlaqə</h3>
        <p class="contact-page-message mb-25">@item.ContactUs</p>
        <div class="single-contact-block">
            <h4><i class="fa fa-fax"></i> Ünvan</h4>
            <p>@item.Location</p>
        </div>
        <div class="single-contact-block">
            <h4><i class="fa fa-phone"></i> Telefon</h4>
            <p>@item.PhoneNumber</p>
        </div>
        <div class="single-contact-block last-child">
            <h4><i class="fa fa-envelope-o"></i> Email</h4>
            <p>@item.EmailAddress</p>
        </div>
    </div>
}
