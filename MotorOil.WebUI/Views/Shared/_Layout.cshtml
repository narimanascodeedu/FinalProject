    <script>
        $(document).ready(function () {


            $('#newsletter-subscribe').submit(function (e) {
                e.preventDefault();

                let formData = new FormData(e.currentTarget);

                $.ajax({
                    url: '@Url.Action("Subscribe")',
                    type: 'POST',
                    data: formData,
                    processData: false,
                    contentType:false,
                    success: function (response) {

                if (response.error == true)
                {
                    if (response.message == "Email düzgün deyil")
                    {
                        toastr.info("name@example.com",response.message);
                        return;
                    }

                    toastr.error(response.message);



                    //showErrors(response.state);
                    return;
                }
                if (response.message == "Siz artiq abune olmusunuz")
                {
                    toastr.warning(response.message);
                    e.currentTarget.reset();
                    return;
                }
                toastr.success(response.message)

                e.currentTarget.reset();
            },
            error: function (errorResponse) {
                console.log(errorResponse,'ERROR')
            }
                });
            });
        });
    </script>

